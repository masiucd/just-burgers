{
  "name": "gatsby-typescript-emotion-storybook-starter",
  "author": "Duncan Leung <leungd@gmail.com> (https://www.duncanleung.com)",
  "description": "Gatsby + TypeScript + Emotion + Storybook + React Intl + SVGR + Jest - Starter",
  "version": "1.0.0",
  "homepage": "https://github.com/duncanleung/gatsby-storybook-typescript-emotion-react-intl-svgr",
  "bugs": "https://github.com/duncanleung/gatsby-storybook-typescript-emotion-react-intl-svgr",
  "keywords": [
    "gatsby",
    "typescript",
    "emotion",
    "storybook",
    "react-intl",
    "svgr",
    "jest"
  ],
  "license": "MIT",
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setup-test-env.js"
  },
  "scripts": {
    "build": "gatsby build",
    "clean": "rm -rf public && rm -rf .cache",
    "dev": "yarn clean && node --max-http-header-size=16385 node_modules/.bin/gatsby develop",
    "debug": "yarn clean && node --nolazy --inspect-brk node_modules/.bin/gatsby develop",
    "serve": "node --max-http-header-size=16385 node_modules/.bin/gatsby serve",
    "lint": "yarn lint:js && yarn lint:ts",
    "lint:js": "./node_modules/.bin/eslint --color --ext .js,.jsx .",
    "lint:ts": "./node_modules/.bin/eslint --color --ext .ts,.tsx .",
    "eslint-prettier-check-all": "yarn eslint-prettier-check-ts && yarn eslint-prettier-check-js",
    "eslint-prettier-check-ts": "eslint --print-config src/pages/index.tsx | eslint-config-prettier-check",
    "eslint-prettier-check-js": "eslint --print-config gatsby-browser.js | eslint-config-prettier-check",
    "test": "jest",
    "test:coverage": "yarn test --coverage",
    "test:watch": "jest --watch",
    "format": "yarn lint:js --fix && yarn lint:ts --fix",
    "storybook": "start-storybook",
    "storybook:build": "build-storybook -c .storybook -o .out"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@reach/router": "^1.3.1",
    "@svgr/webpack": "^5.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.19.19",
    "gatsby-image": "^2.2.41",
    "gatsby-plugin-emotion": "^4.1.22",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-intl": "0.3.3",
    "gatsby-plugin-react-helmet": "^3.1.22",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-plugin-svgr": "^2.0.2",
    "gatsby-plugin-typescript": "^2.1.27",
    "gatsby-transformer-sharp": "^2.3.14",
    "polished": "^3.4.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-info": "^5.3.13",
    "@storybook/addon-viewport": "^5.3.13",
    "@storybook/preset-typescript": "^1.2.0",
    "@storybook/react": "^5.3.13",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^25.1.3",
    "@types/node": "^13.7.4",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "@types/react-intl": "2.3.18",
    "@types/storybook__react": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.27",
    "babel-plugin-jsx-remove-data-test-id": "^2.1.3",
    "babel-plugin-remove-graphql-queries": "^2.7.23",
    "babel-preset-gatsby": "^0.2.29",
    "babel-preset-react-app": "^9.1.1",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-no-only-tests": "^2.4.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^2.4.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.8",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "jest-emotion": "^10.0.27",
    "jest-localstorage-mock": "^2.4.0",
    "precise-commits": "^1.0.2",
    "prettier": "^1.19.1",
    "react-docgen-typescript-loader": "^3.6.0",
    "react-test-renderer": "^16.12.0",
    "storybook-addon-intl": "^2.4.1",
    "ts-essentials": "^6.0.1",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "precise-commits"
    }
  }
}
